{
  "version": "2.0.0",
  "tasks": [
    {
      "taskName": "build",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "${workspaceFolder}/src/WebSite/webSite.csproj"
      ],
      "problemMatcher": "$msCompile",
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },    
    {
      "taskName": "test",
      "command": "dotnet",
      "type": "process",
      "args": [
        "test",
        "${workspaceFolder}/test/DSL_Tests/DSL_Tests.csproj"
      ],
      "options": {
        "cwd": "${workspaceFolder}/src/WebSite"
      },
      "problemMatcher": "$msCompile",
      "group": {
        "kind": "test",
        "isDefault": true
      }
    }

  ]
}
