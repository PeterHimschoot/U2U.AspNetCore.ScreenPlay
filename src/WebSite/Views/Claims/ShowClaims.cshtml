@model WebSite.ViewModels.Claims.ShowViewModel

@{
    ViewData["Title"] = "Claims";
}

<h2>Claims</h2>

<table class="table">
  <thead>
    <tr>
      <th>Claim</th>
    </tr>
  </thead>
  <tbody>
    @foreach (var item in Model.Claims) {
    <tr>
      <td>
        @item
      </td>
    </tr>
    }
  </tbody>
</table>
